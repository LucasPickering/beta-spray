  "scripts": {
    "start": "webpack serve",
    "prebuild": "npm run relay",
    "build": "NODE_ENV=production webpack",
    "check": "npm run type-check && npm run lint",
    "analyze": "NODE_ENV=production WEBPACK_BUNDLE_ANALYZER_MODE=server webpack",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf node_modules **/__generated__",
    "test": "jest",
    "pretype-check": "npm run relay",
    "type-check": "tsc",
    "relay": "relay-compiler",
    "relay:watch": "nodemon -x \"npm run relay\" -w src -e \"ts tsx\" -i \"**/__generated/*\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
