  "scripts": {
    "start": "webpack serve",
    "prebuild": "npm run clean && npm run relay",
    "build": "NODE_ENV=production webpack",
    "check": "npm run type-check && npm run lint",
    "analyze": "WEBPACK_BUNDLE_ANALYZER_MODE=server npm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf build/",
    "test": "jest",
    "pretype-check": "npm run relay",
    "type-check": "tsc",
    "relay": "relay-compiler",
    "relay:watch": "nodemon -x \"npm run relay\" -w src -e \"ts tsx\" -i \"**/__generated/*\""
